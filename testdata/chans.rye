example: import\go "example.com"

do\par example {
    ch: Get1And2
    print ch .read
    print ch .read

    done: channel 0

    Printer done
        |send 10
        |send 20
        |close
    done .read

    dch: Doubler done
    dch .send 15
    print dch .read
    dch .send 35
    print dch .read
    dch .close
    done .read

    pch: channel 0
    go does { PrintChan pch }
    pch
        |send 9
        |send 8
        |send 7
        |close

    try {
        Printer done
            |send "abc"
            |close
        done .read
    }
}