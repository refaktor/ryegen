example: import\go "example.com"

do\par example {
    ch: Get1And2
    print ch .Read
    print ch .Read

    done: channel 0

    Printer done
        |Send 10
        |Send 20
        |Close
    done .Read

    dch: Doubler done
    dch .Send 15
    print dch .Read
    dch .Send 35
    print dch .Read
    dch .Close
    done .Read

    pch: channel 0
    go does { PrintChan pch }
    pch
        |Send 9
        |Send 8
        |Send 7
        |Close

    try {
        Printer done
            |Send "abc"
            |Close
        done .Read
    }
}